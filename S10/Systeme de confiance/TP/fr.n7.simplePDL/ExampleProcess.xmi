<?xml version="1.0" encoding="UTF-8"?>
<simplepdl:Process
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:simplepdl="http://simplepdl"
    xsi:schemaLocation="http://simplepdl SimplePDL.ecore"
    name="Développement Application Web">
  <activities
      linksToSuccessors="//@workSequences.0"
      name="Analyse"/>
  <activities
      linksToPredecessors="//@workSequences.0"
      linksToSuccessors="//@workSequences.1"
      name="Conception"/>
  <activities
      linksToPredecessors="//@workSequences.1"
      linksToSuccessors="//@workSequences.2"
      name="Codage"/>
  <activities
      linksToPredecessors="//@workSequences.2"
      linksToSuccessors="//@workSequences.3"
      name="Tests"/>
  <activities
      linksToPredecessors="//@workSequences.3"
      name="Déploiement"/>
  <workSequences
      linkType="finishToStart"
      predecessor="//@activities.0"
      successor="//@activities.1"/>
  <workSequences
      linkType="finishToStart"
      predecessor="//@activities.1"
      successor="//@activities.2"/>
  <workSequences
      linkType="finishToStart"
      predecessor="//@activities.2"
      successor="//@activities.3"/>
  <workSequences
      linkType="finishToStart"
      predecessor="//@activities.3"
      successor="//@activities.4"/>
</simplepdl:Process>
